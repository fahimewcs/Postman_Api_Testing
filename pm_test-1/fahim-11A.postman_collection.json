	"item": [
		{
			"name": "Create Booking",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = pm.response.json();\r",
							"pm.environment.set(\"ID\",jsonData.bookingid)\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"First name check\", function () {\r",
							"    pm.expect(\"Jima\").to.eql(jsonData.booking.firstname);\r",
							"});\r",
							"\r",
							"pm.test(\"Last name check\", function () {\r",
							"    pm.expect(jsonData.booking.lastname).to.eql(\"Brown\");\r",
							"});\r",
							"\r",
							"pm.test(\"total price check\", function () {\r",
							"    pm.expect(1123).to.eql(jsonData.booking.totalprice);\r",
							"});\r",
							"\r",
							"pm.test(\"deposit status check\", function () {\r",
							"    pm.expect(true).to.eql(jsonData.booking.depositpaid);\r",
							"});\r",
							"\r",
							"pm.test(\"Checkin Date check\", function () {\r",
							"    pm.expect(\"2021-01-01\").to.eql(jsonData.booking.bookingdates.checkin);\r",
							"});\r",
							"\r",
							"pm.test(\"Checkout Date check\", function () {\r",
							"    pm.expect(\"2022-01-01\").to.eql(jsonData.booking.bookingdates.checkout);\r",
							"});\r",
							"\r",
							"pm.test(\"Additional needs check\", function () {\r",
							"    pm.expect(\"Breakfast\").to.eql(jsonData.booking.additionalneeds);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
